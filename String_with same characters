
import java.lang.*;
import java.util.*;
class Main {
    public static void main(String[] args) {
       String str1 = "dog";
       String str2 = "god";
       
       char[] charStr1 = str1.toCharArray();
       char[] charStr2 = str2.toCharArray();
       
       Arrays.sort(charStr1);
       Arrays.sort(charStr2);
       
       System.out.println(Arrays.toString(charStr1 )+ " " + Arrays.toString(charStr2));
       
       
         if(Arrays.equals(charStr1, charStr2)){
               System.out.println(str1 + " and " + str2 + " are made with same character");
           } else {
                System.out.println(str1 + " and " + str2 + " are made different character");
           }
      
    }
    
}

/*
Output: 
[d, g, o] [d, g, o]
dog and god are made with same character
*/
