
import java.lang.*;
import java.util.*;
class Main {
    public static void main(String[] args) {
        String name = "Deepak Sharma";
        String str1 = "Deepak" ;
        str1.concat("Sharma");
        
        if(name == str1){
            System.out.println("Matched hence mutable");
        } else {
            System.out.println("Not matched hence Immutable");
        }
    }
    
}
