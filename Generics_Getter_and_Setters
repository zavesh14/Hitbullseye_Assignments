class Generics<T>{
    private T value;
    public Generics(){
        
    }
    public Generics(T value){
        this.value = value;
    }
    
    public T getValue(){
        return value;
    }
    public void setValue(T value){
        this.value = value;
    }
}
class Main{
    public static void main(String[] args) {
        //  Generics<Integer> test = new Generics<>(50);
        Generics<Integer> test = new Generics<>();
        
        // System.out.println(test.getValue());
        test.setValue(70);
        System.out.println(test.getValue());
    }
}
